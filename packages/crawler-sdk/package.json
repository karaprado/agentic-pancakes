{
  "name": "@agent-ready-web/crawler-sdk",
  "version": "1.0.0",
  "description": "Comprehensive TypeScript SDK for Agent Ready Web crawler service",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "src",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts --clean",
    "build:watch": "tsup src/index.ts --format cjs,esm --dts --watch",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src tests examples --ext .ts",
    "lint:fix": "eslint src tests examples --ext .ts --fix",
    "typecheck": "tsc --noEmit",
    "format": "prettier --write \"src/**/*.ts\" \"tests/**/*.ts\" \"examples/**/*.ts\"",
    "prepublishOnly": "npm run build",
    "example:basic": "ts-node examples/basic-usage.ts",
    "example:arw": "ts-node examples/arw-discovery.ts",
    "example:stream": "ts-node examples/streaming.ts",
    "example:batch": "ts-node examples/batch-operations.ts"
  },
  "keywords": [
    "crawler",
    "scraper",
    "web-scraping",
    "agent-ready-web",
    "arw",
    "llms-txt",
    "machine-view",
    "sdk",
    "typescript",
    "firecrawl"
  ],
  "author": "Agent Ready Web Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/agent-ready-web/agent-ready-web.git",
    "directory": "packages/crawler-sdk"
  },
  "bugs": {
    "url": "https://github.com/agent-ready-web/agent-ready-web/issues"
  },
  "homepage": "https://github.com/agent-ready-web/agent-ready-web#readme",
  "dependencies": {
    "axios": "^1.6.0",
    "ws": "^8.14.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.0",
    "@types/node": "^20.10.0",
    "@types/ws": "^8.5.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.50.0",
    "jest": "^29.7.0",
    "prettier": "^3.0.0",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.0",
    "tsup": "^8.0.0",
    "typescript": "^5.3.0"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
