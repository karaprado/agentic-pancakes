{
  "name": "@agent-ready-web/cli",
  "version": "0.2.0",
  "description": "High-performance ARW CLI with native Node.js bindings",
  "main": "index.js",
  "types": "index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/agent-ready-web/agent-ready-web.git",
    "directory": "packages/cli"
  },
  "license": "MIT",
  "keywords": [
    "arw",
    "agent-ready-web",
    "ai",
    "agents",
    "llms",
    "napi-rs",
    "rust",
    "performance"
  ],
  "engines": {
    "node": ">= 10"
  },
  "napi": {
    "name": "arw-cli",
    "triples": {
      "defaults": true,
      "additional": [
        "aarch64-apple-darwin",
        "aarch64-unknown-linux-gnu",
        "aarch64-unknown-linux-musl",
        "armv7-unknown-linux-gnueabihf"
      ]
    }
  },
  "scripts": {
    "artifacts": "napi artifacts",
    "build": "napi build --platform --release --features napi",
    "build:debug": "napi build --platform --features napi",
    "build:wasm": "wasm-pack build --target nodejs --out-dir wasm-pkg/nodejs --no-default-features --features wasm",
    "build:wasm:web": "wasm-pack build --target web --out-dir wasm-pkg/web --no-default-features --features wasm",
    "build:wasm:bundler": "wasm-pack build --target bundler --out-dir wasm-pkg/bundler --no-default-features --features wasm",
    "pretest": "npm run build:wasm",
    "prepublishOnly": "napi prepublish -t npm",
    "test": "node --test",
    "test:wasm": "node wasm-tests/validate.test.js && node wasm-tests/generate.test.js && node wasm-tests/performance.test.js",
    "version": "napi version"
  },
  "devDependencies": {
    "@napi-rs/cli": "^2.18.0"
  },
  "optionalDependencies": {
    "@agent-ready-web/cli-darwin-x64": "0.2.0",
    "@agent-ready-web/cli-darwin-arm64": "0.2.0",
    "@agent-ready-web/cli-linux-x64-gnu": "0.2.0",
    "@agent-ready-web/cli-linux-x64-musl": "0.2.0",
    "@agent-ready-web/cli-linux-arm64-gnu": "0.2.0",
    "@agent-ready-web/cli-linux-arm64-musl": "0.2.0",
    "@agent-ready-web/cli-win32-x64-msvc": "0.2.0",
    "@agent-ready-web/cli-win32-arm64-msvc": "0.2.0"
  }
}
